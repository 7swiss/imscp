msgid ""
"'%hostname%' does not appear to have a valid MX record for the email address"
" '%value%'"
msgstr "'%hostname%' имеет неверную MX запись для электронного адреса '%value%'"

msgid "'%hostname%' is no valid hostname for email address '%value%'"
msgstr "'%hostname%' - неверное имя хоста для электронного адреса '%value%'"

msgid ""
"'%hostname%' is not in a routable network segment. The email address "
"'%value%' should not be resolved from public network"
msgstr "'%hostname%' - немаршрутизируемый сегмент сети. Электронный адрес '%value%' не будет доступен из всемирной сети"

#, php-format
msgid "'%localPart%' can not be matched against dot-atom format"
msgstr "'%localPart%' не відповідає формату dot-atom"

#, php-format
msgid "'%localPart%' can not be matched against quoted-string format"
msgstr "%localPart%' не відповідає формату quoted-string"

msgid "'%localPart%' is no valid local part for email address '%value%'"
msgstr "'%localPart%' неверная локальная часть для электронного адреса '%value%'"

msgid "'%value%' appears to be a DNS hostname but cannot extract TLD part"
msgstr "'%value%' является DNS именем узла, но не может извлечь TLD часть"

msgid ""
"'%value%' appears to be a DNS hostname but cannot match TLD against known "
"list"
msgstr "'%value%'  является DNS именем узла, но оно не дожно быть из TLD-списка"

msgid ""
"'%value%' appears to be a DNS hostname but cannot match against hostname "
"schema for TLD '%tld%'"
msgstr "'%value%'  является DNS именем узла, но не совпадает со схемой имен узла для TLD '%tld%'"

msgid ""
"'%value%' appears to be a DNS hostname but contains a dash in an invalid "
"position"
msgstr "'%value%'  является DNS именем узла, но знак '-' находится в неправильном месте"

msgid ""
"'%value%' appears to be a DNS hostname but the given punycode notation "
"cannot be decoded"
msgstr "'%value%' является DNS именем узла, но данный punycode не может быть раскодирован"

msgid "'%value%' appears to be a domain name but cannot extract TLD part"
msgstr "'%value%' является доменным именем, но невозможно извлечь TLD часть"

msgid ""
"'%value%' appears to be a domain name but cannot match TLD against known "
"list"
msgstr "'%value%' является доменным именем, но оно не дожно быть из TLD-списка"

msgid ""
"'%value%' appears to be a domain name but cannot match against domain name "
"schema for TLD '%tld%'"
msgstr "'%value%' является доменным именем, но не совпадает со схемой имен узла для TLD '%tld%'"

msgid ""
"'%value%' appears to be a domain name but contains a dash in an invalid "
"position"
msgstr "'%value%' является доменным именем, но знак '-' находится в неправильном месте"

msgid ""
"'%value%' appears to be a domain name but the given punycode notation cannot"
" be decoded"
msgstr "'%value%' является доменным именем, но данный punycode не может быть раскодирован"

msgid ""
"'%value%' appears to be a local network name but local network names are not"
" allowed"
msgstr "'%value%' является именем сети, но локалькые сетевые имена не разрешены "

