<?xml version="1.0" encoding="UTF-8"?>
<install>
    <folders mode="0755">/etc/init</folders>
    <copy_config mode="0644" expand_vars="1">/etc/init/imscp_daemon.conf</copy_config>
    <copy_config mode="0644" expand_vars="1">/etc/init/imscp_mountall.conf</copy_config>
    <copy_config if="-f '/etc/init/php5.6-fpm.conf'" mode="0644">/etc/init/php5.6-fpm.override</copy_config>
    <copy_config if="-f '/etc/init/php7.0-fpm.conf'" mode="0644">/etc/init/php7.0-fpm.override</copy_config>
    <copy_config if="-f '/etc/init/php7.1-fpm.conf'" mode="0644">/etc/init/php7.1-fpm.override</copy_config>
    <copy_config if="-f '/etc/init/php7.2-fpm.conf'" mode="0644">/etc/init/php7.2-fpm.override</copy_config>
    <copy_config if="-f '/etc/init/php7.3-fpm.conf'" mode="0644">/etc/init/php7.3-fpm.override</copy_config>
    <copy_config mode="0644" expand_vars="1">/etc/init/imscp_traffic.conf</copy_config>
</install>
