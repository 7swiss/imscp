{
  "name": "imscp/frontend",
  "description": "i-MSCP internet Multi-Server Control Panel - FrontEnd",
  "type": "project",
  "keywords": [
    "imscp",
    "frontend"
  ],
  "homepage": "https://www.i-mscp.net/",
  "license": "LGPL-2.1-only",
  "support": {
    "issues": "https://youtrack.i-mscp.net/",
    "source": "https://github.com/i-MSCP/imscp/"
  },
  "authors": [
    {
      "name": "Laurent Declercq",
      "email": "l.declercq@nuxwin.com",
      "homepage": "https://www.i-mscp.net/",
      "role": "developer"
    }
  ],
  "require": {
    "php": ">=5.6",
    "zendframework/zend-authentication": "^2.0",
    "zendframework/zend-cache": "^2.0",
    "zendframework/zend-escaper": "^2.0",
    "zendframework/zend-eventmanager": "^2.0",
    "zendframework/zend-filter": "^2.0",
    "zendframework/zend-form": "^2.0",
    "zendframework/zend-i18n": "^2.0",
    "zendframework/zend-navigation": "^2.0",
    "zendframework/zend-session": "^2.0",
    "zendframework/zend-uri": "^2.0",
    "zendframework/zend-validator": "^2.0",
    "mso/idna-convert": "^1.0",
    "pear/net_dns2": "^1.0",
    "phpseclib/phpseclib": "^2.0"
  },
  "config": {
    "classmap-authoritative": false,
    "discard-changes": true,
    "htaccess-protect": false,
    "optimize-autoloader": true,
    "apcu-autoloader": true,
    "preferred-install": "dist",
    "process-timeout": 2000
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "include-path": [
    "library/include"
  ],
  "autoload": {
    "psr-4": {
      "iMSCP\\": "library/classes"
    }
  }
}
