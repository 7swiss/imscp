{
  "name": "imscp/frontend",
  "description": "i-MSCP internet Multi-Server Control Panel - FrontEnd",
  "type": "project",
  "keywords": [
    "i-mscp",
    "frontend"
  ],
  "homepage": "https://www.i-mscp.net/",
  "license": "LGPL-2.1-only",
  "support": {
    "issues": "https://youtrack.i-mscp.net/",
    "source": "https://github.com/i-MSCP/imscp/"
  },
  "authors": [
    {
      "name": "Laurent Declercq",
      "email": "l.declercq@nuxwin.com",
      "homepage": "https://www.i-mscp.net/",
      "role": "developer"
    }
  ],
  "require": {
    "php": "^7.1",
    "container-interop/container-interop": "^1.2",
    "mso/idna-convert": "^v1.1",
    "pear/net_dns2": "^v1.4",
    "phpseclib/phpseclib": "^2.0",
    "psr/cache": "^1.0",
    "psr/container": "^1.0",
    "psr/simple-cache": "^1.0",
    "zendframework/zend-authentication": "^2.6",
    "zendframework/zend-cache": "^2.8",
    "zendframework/zend-config": "^3.2",
    "zendframework/zend-db": "^2.9",
    "zendframework/zend-escaper": "^2.6",
    "zendframework/zend-eventmanager": "^3.2",
    "zendframework/zend-filter": "^2.8",
    "zendframework/zend-form": "^2.12",
    "zendframework/zend-http": "^2.8",
    "zendframework/zend-i18n": "^2.9",
    "zendframework/zend-inputfilter": "^2.8",
    "zendframework/zend-math": "^2.7",
    "zendframework/zend-navigation": "^2.9",
    "zendframework/zend-serializer": "^2.9",
    "zendframework/zend-servicemanager": "^3.3",
    "zendframework/zend-session": "^2.8",
    "zendframework/zend-stdlib": "^3.2",
    "zendframework/zend-uri": "^2.6",
    "zendframework/zend-validator": "^2.10"
  },
  "config": {
    "classmap-authoritative": false,
    "discard-changes": true,
    "htaccess-protect": false,
    "optimize-autoloader": false,
    "apcu-autoloader": false,
    "preferred-install": "dist",
    "process-timeout": 2000
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "include-path": [
    "library/include/"
  ],
  "autoload": {
    "psr-4": {
      "iMSCP\\": "library/classes",
      "iMSCP\\Plugin\\": "plugins"
    },
    "files": [
      "library/include/functions.php"
    ]
  }
}
