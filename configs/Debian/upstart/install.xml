<?xml version="1.0" encoding="UTF-8"?>
<install umask="0022">
    <folder>${ROOT_CONF}/init</folder>
    <copy_config>${ROOT_CONF}/init/imscp_daemon.conf</copy_config>
    <copy_config>${ROOT_CONF}/init/imscp_mountall.conf</copy_config>
    <copy_config>${ROOT_CONF}/init/imscp_panel.conf</copy_config>
    <copy_config>${ROOT_CONF}/init/imscp_panel.override</copy_config>
    <copy_config copy_if="-f '/etc/init/php5.6-fpm.conf'">${ROOT_CONF}/init/php5.6-fpm.override</copy_config>
    <copy_config copy_if="-f '/etc/init/php7.0-fpm.conf'">${ROOT_CONF}/init/php7.0-fpm.override</copy_config>
    <copy_config copy_if="-f '/etc/init/php7.1-fpm.conf'">${ROOT_CONF}/init/php7.1-fpm.override</copy_config>
    <copy_config copy_if="-f '/etc/init/php7.2-fpm.conf'">${ROOT_CONF}/init/php7.2-fpm.override</copy_config>
    <copy_config>${ROOT_CONF}/init/imscp_traffic.conf</copy_config>
</install>
